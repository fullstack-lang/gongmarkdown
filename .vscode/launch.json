{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-fullstack-lang-gongmarkdown"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:4200",
            "webRoot": "${workspaceFolder}/ng-github.com-fullstack-lang-gongmarkdown"
        },
        {
            "name": "go server -marshallOnCommit=stage",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/gongmarkdown",
            "args": [
                "-marshallOnCommit=data/stage",
                "-generateDocument"
            ]
        },
        {
            "name": "go markdown",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/go/cmd/gongmarkdown/markdown",
            "args": [
            ]
        },
    ]
}
